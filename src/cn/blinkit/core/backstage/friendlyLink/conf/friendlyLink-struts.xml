<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="friendlyLink-action" namespace="/admin/link" extends="base-default">
		<action name="link_*" class="cn.blinkit.core.backstage.friendlyLink.action.friendlyLinkAction" method="{1}">
			<result name="{1}">/WEB-INF/jsp/backstage/user/{1}.jsp</result>
			<result name="list" type="redirectAction">
				<param name="actionName">link_listUI</param>
				<param name="user.name">${strName}</param>
				<param name="encode">true</param>
			</result>
			
		</action>
	</package>

</struts>
